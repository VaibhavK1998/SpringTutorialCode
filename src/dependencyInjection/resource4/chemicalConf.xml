<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"> 
        <bean class="dependencyInjection.deepBeans.Chemicals_DIInjectionViaP_nameSpace" id="chemical"
        p:NameOfChemical="methanol"
        p:priceOfchemical="100"
      />
</beans>
 <!-- note we have to write xmlns:p="http://www.springframework.org/schema/p" this declaration line to use p-namespace -->